@model Spice.Models.Subcategory
@{
    ViewData["Title"] = "delete";
}
<br />
<h2 class=" text-danger">Are You Sure To delete </h2>

<br />

<form asp-action="delete">
    <div class="form-border divcolor">
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="name" class="custom-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="name" readonly class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="category.name" class="custom-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="category.name" readonly class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2"></div>
            <div class="col-2">

                <input type="submit" class="btn btn-danger form-control" value="Delete" />
            </div>
            <div class="col-3">

                <a asp-action="index" class="btn btn-success form-control">Back To List</a>
            </div>
        </div>
    </div>
</form>